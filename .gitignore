# never ignore the following files
!.gitattributes
!.gitkeep

# ignore Eclipse directories and files
/.externalToolBuilders/
/.settings/
/.buildpath
/.project*

# ignore the following directories and files
/bin/debugview/dbgview.*
/bin/experts
/etc/windows/user.appdata/*
/files/
mqlcache.dat
*.ex4
*.ex5

# ignore distributed/linked/log folders
/mql4.*/files
/mql4.*/images
/mql4.*/logs
/mql4.*/presets
/mql4.*/projects
/mql4.*/shared projects

# ignore files created by the tester
/tester/
